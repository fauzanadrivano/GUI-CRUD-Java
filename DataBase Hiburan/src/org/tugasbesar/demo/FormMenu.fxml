<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="375.0" prefWidth="523.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.tugasbesar.demo.FormMenuController">
   <children>
      <ImageView fitHeight="375.0" fitWidth="523.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@bg4.png" />
         </image>
      </ImageView>
      <Button fx:id="btnGotoFilm" layoutX="161.0" layoutY="107.0" mnemonicParsing="false" onAction="#switchToFilm" prefHeight="60.0" prefWidth="201.0" text="Go to Database Film" />
      <Button fx:id="btnGotoMusic" layoutX="161.0" layoutY="195.0" mnemonicParsing="false" onAction="#switchToMusic" prefHeight="60.0" prefWidth="201.0" text="Go to Database Music" />
      <Button fx:id="btnBack" layoutX="100.0" layoutY="284.0" mnemonicParsing="false" onAction="#back" prefHeight="36.0" prefWidth="123.0" text="Back" />
      <Button fx:id="btnExit" layoutX="301.0" layoutY="284.0" mnemonicParsing="false" onAction="#exit" prefHeight="36.0" prefWidth="123.0" text="Exit" textFill="RED" />
      <Label layoutX="221.0" layoutY="35.0" text="MENU" textFill="WHITE">
         <font>
            <Font name="System Bold" size="27.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
